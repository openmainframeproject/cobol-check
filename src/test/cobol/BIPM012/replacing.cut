TESTSUITE 'TEST SUITE FOR REPLACE  '

TESTCASE 'TEST CASE #0 - WS FIELD - ALFANUM'

  MOVE '1970-01-01'       TO WS-DATE-BEFORE
  PERFORM 100-MOVE-DATA
  EXPECT WS-DATE-AFTER    TO BE '1970-01-01'


TESTCASE 'TEST CASE #0 - WS FIELD - NUM'

  MOVE 53                 TO MOVE-IDX
  PERFORM 100-MOVE-DATA
  EXPECT MOVE-IDX         TO BE NUMERIC 53


TESTCASE 'TEST CASE #1.1 - COPYBOOK FIELD - POSITIVE VALUE '

  MOVE 1   TO MAX-LENGTH     IN :BDSIXXX:-PARM
  PERFORM 100-MOVE-DATA
  EXPECT MAX-LENGTH          IN :BDSIXXX:-PARM
      TO BE NUMERIC 1


TESTCASE 'TEST CASE #1.2 - COPYBOOK FIELD - NEGATIV VALUE '

  MOVE -5   TO MAX-LENGTH    IN :BDSIXXX:-PARM
  PERFORM 100-MOVE-DATA
  EXPECT MAX-LENGTH          IN :BDSIXXX:-PARM
      TO BE NUMERIC -5
  .